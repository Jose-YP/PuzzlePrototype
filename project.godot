config_version=5

[application]

config/name="PuzzlePrototype"
config/tags=PackedStringArray("2d", "made")
run/main_scene="res://Scenes/MainMenu/game_manager.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://Art/Beads/LiquidOrb.png"

[autoload]

Globals="*res://Code/Globals/Globals.gd"
NG="*res://addons/newgrounds/scripts/newgrounds.tscn"
NGCloudSave="*res://addons/newgrounds/scripts/newgrounds_cloudsave.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/always_on_top=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/newgrounds/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "red",
"res://Audio/": "red",
"res://Code/": "green",
"res://Resources/": "green",
"res://Scenes/": "green",
"res://Shaders/": "pink",
"res://Themes/": "pink",
"res://addons/": "orange"
}

